{
  "name": "ShareGPT",
  "description": "Share your ChatGPT conversations",
  "version": "1.0.5",
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/chat"],
      "js": ["index.js"],
      "run_at": "document_end"
    }
  ]
}
